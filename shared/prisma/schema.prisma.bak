// Orphaned Records Model - Tracks records that couldn't be migrated due to missing relationships
model OrphanedRecord {
  id                String            @id @default(cuid())

  // Source identification
  salesforceId      String            @map("salesforce_id")
  salesforceType    String            @map("salesforce_type") // WorkOrder, ServiceAppointment, Quote, etc.
  recordNumber      String?           @map("record_number") // WorkOrderNumber, QuoteNumber, etc.
  recordName        String?           @map("record_name")

  // What's missing
  orphanReason      OrphanReason      @map("orphan_reason")
  missingFieldName  String?           @map("missing_field_name") // AccountId, ContactId, OpportunityId, etc.
  missingFieldValue String?           @map("missing_field_value") // The actual null or invalid value

  // Resolution status
  status            OrphanStatus      @default(PENDING) @map("status")
  resolvedAt        DateTime?         @map("resolved_at")
  resolvedById      String?           @map("resolved_by_id")
  resolvedAction    String?           @map("resolved_action") // LINKED, DELETED, SKIPPED

  // Linked record after resolution
  linkedRecordId    String?           @map("linked_record_id")
  linkedRecordType  String?           @map("linked_record_type")

  // Original Salesforce data for reference
  salesforceData    Json?             @map("salesforce_data")

  // Migration context
  migrationRunId    String?           @map("migration_run_id")

  createdAt         DateTime          @default(now()) @map("created_at")
  updatedAt         DateTime          @updatedAt @map("updated_at")

  // Relations
  resolvedBy        User?             @relation("OrphanResolvedBy", fields: [resolvedById], references: [id])
  migrationRun      MigrationRun?     @relation("MigrationOrphans", fields: [migrationRunId], references: [id])

  @@unique([salesforceId, salesforceType])
  @@index([salesforceType])
  @@index([orphanReason])
  @@index([status])
  @@index([migrationRunId])
  @@map("orphaned_records")
}

enum OrphanReason {
  NULL_ACCOUNT_ID      // AccountId is null
  NULL_CONTACT_ID      // ContactId is null
  NULL_OPPORTUNITY_ID  // OpportunityId is null
  INVALID_ACCOUNT_ID   // AccountId doesn't exist in CRM
  INVALID_CONTACT_ID   // ContactId doesn't exist in CRM
  INVALID_OPPORTUNITY_ID // OpportunityId doesn't exist in CRM
  MISSING_PARENT       // Parent record doesn't exist
  OTHER                // Other reason
}

enum OrphanStatus {
  PENDING              // Not yet reviewed
  REVIEWING            // Under review
  LINKED               // Manually linked to a record
  DELETED              // Marked as deleted/not needed
  SKIPPED              // Intentionally skipped
}
