  fileExtension              String?        @map("file_extension")
  contentSize                Int?           @map("content_size")
  contentUrl                 String?        @map("content_url") // Salesforce ContentVersion URL
  versionNumber              Int?           @map("version_number")
  isArchived                 Boolean        @default(false) @map("is_archived")
  archivedDate               DateTime?      @map("archived_date")
  archivedById               String?        @map("archived_by_id")
  latestVersionSalesforceId  String?        @map("latest_version_salesforce_id")
  ownerId                    String?        @map("owner_id")
