// ============================================================================
// MIGRATION AUDIT MODELS
// ============================================================================

// Track each migration run for audit and rollback purposes
model MigrationRun {
  id              String              @id @default(cuid())
  runId           String              @unique @map("run_id") // e.g., "migration-1736XXX"
  startedAt       DateTime            @default(now()) @map("started_at")
  completedAt     DateTime?           @map("completed_at")
  status          MigrationRunStatus  @default(RUNNING)

  // Configuration
  dryRun          Boolean             @default(false) @map("dry_run")
  objectsToSync   String[]            @default([]) @map("objects_to_sync")

  // Statistics
  totalRecords    Int                 @default(0) @map("total_records")
  matchedRecords  Int                 @default(0) @map("matched_records")
  createdRecords  Int                 @default(0) @map("created_records")
  updatedRecords  Int                 @default(0) @map("updated_records")
  skippedRecords  Int                 @default(0) @map("skipped_records")
  errorRecords    Int                 @default(0) @map("error_records")

  // Error tracking
  errors          Json?               // Array of error details

  // Relations
  auditLogs       MigrationAuditLog[]
  mappings        MigrationMapping[]
  orphanedRecords OrphanedRecord[]    @relation("MigrationOrphans")

  @@index([status])
  @@index([startedAt])
  @@map("migration_runs")
}

// Detailed audit log for each record processed during migration
model MigrationAuditLog {
  id                  String                  @id @default(cuid())
  runId               String                  @map("run_id")

  // Record identification
  objectType          String                  @map("object_type") // accounts, contacts, leads, opportunities
  salesforceId        String?                 @map("salesforce_id")
  crmRecordId         String?                 @map("crm_record_id")

  // Action taken
  action              MigrationAction
  matchMethod         String?                 @map("match_method") // explicit_id, email, phone, composite, fuzzy
  matchConfidence     Float?                  @map("match_confidence")

  // Change details
  fieldsChanged       String[]                @default([]) @map("fields_changed")
  oldValues           Json?                   @map("old_values")
  newValues           Json?                   @map("new_values")

  // Status and notes
  status              MigrationRecordStatus   @default(SUCCESS)
  errorMessage        String?                 @map("error_message")
  requiresReview      Boolean                 @default(false) @map("requires_review")
  reviewReason        String?                 @map("review_reason")

  createdAt           DateTime                @default(now()) @map("created_at")

  // Relations
  migrationRun        MigrationRun            @relation(fields: [runId], references: [runId], onDelete: Cascade)

  @@index([runId])
  @@index([objectType])
  @@index([salesforceId])
  @@index([crmRecordId])
  @@index([action])
  @@index([requiresReview])
  @@map("migration_audit_logs")
}

// Mapping table to track Salesforce ID  CRM ID relationships
model MigrationMapping {
  id              String    @id @default(cuid())
  runId           String    @map("run_id")

  objectType      String    @map("object_type") // accounts, contacts, leads, opportunities
  salesforceId    String    @map("salesforce_id")
  crmRecordId     String    @map("crm_record_id")

  // Match metadata
  matchMethod     String?   @map("match_method")
  matchConfidence Float?    @map("match_confidence")

  // Audit
  createdAt       DateTime  @default(now()) @map("created_at")
  updatedAt       DateTime  @updatedAt @map("updated_at")

  // Relations
  migrationRun    MigrationRun @relation(fields: [runId], references: [runId], onDelete: Cascade)

  @@unique([objectType, salesforceId])
  @@unique([objectType, crmRecordId])
  @@index([runId])
  @@index([objectType])
  @@map("migration_mappings")
}

// Enums for migration tracking
enum MigrationRunStatus {
  RUNNING
  COMPLETED
  FAILED
  CANCELLED
}

enum MigrationAction {
  MATCHED
  CREATED
  UPDATED
  SKIPPED
  ERROR
  FLAGGED_FOR_REVIEW
}

enum MigrationRecordStatus {
  SUCCESS
  SKIPPED
  ERROR
  NEEDS_REVIEW
}

