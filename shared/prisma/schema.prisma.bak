// PandaCam Photo Project Model
model PhotoProject {
  id                String              @id @default(cuid())
  name              String
  description       String?
  projectType       PhotoProjectType    @default(JOB) @map("project_type")
  status            PhotoProjectStatus  @default(ACTIVE)

  // Address
  street            String?
  city              String?
  state             String?
  postalCode        String?             @map("postal_code")
  country           String?             @default("US")
  latitude          Float?
  longitude         Float?

  // Dates
  startDate         DateTime?           @map("start_date")
  completionDate    DateTime?           @map("completion_date")
  archivedDate      DateTime?           @map("archived_date")

  // Relationships
  opportunityId     String?             @map("opportunity_id")
  accountId         String?             @map("account_id")
  workOrderId       String?             @map("work_order_id")
  ownerId           String?             @map("owner_id")
  archivedById      String?             @map("archived_by_id")

  // Stats (denormalized for performance)
  totalPhotos       Int                 @default(0) @map("total_photos")
  totalVideos       Int                 @default(0) @map("total_videos")
  totalChecklists   Int                 @default(0) @map("total_checklists")
  completionPercent Float?              @map("completion_percent")

  // Metadata
  tags              String[]            @default([])
  metadata          Json?

  createdAt         DateTime            @default(now()) @map("created_at")
  updatedAt         DateTime            @updatedAt @map("updated_at")

  // Relations
  opportunity       Opportunity?        @relation("PhotoProjectOpportunity", fields: [opportunityId], references: [id])
  account           Account?            @relation("PhotoProjectAccount", fields: [accountId], references: [id])
  workOrder         WorkOrder?          @relation("PhotoProjectWorkOrder", fields: [workOrderId], references: [id])
  owner             User?               @relation("PhotoProjectOwner", fields: [ownerId], references: [id])
  archivedBy        User?               @relation("PhotoProjectArchivedBy", fields: [archivedById], references: [id])

  photos            Photo[]
  checklists        PhotoChecklist[]
  pages             PhotoPage[]
  galleries         PhotoGallery[]
  comparisons       BeforeAfterComparison[]
  collaborators     ProjectCollaborator[]
  activities        PhotoProjectActivity[]

  @@index([opportunityId])
  @@index([accountId])
  @@index([workOrderId])
  @@index([ownerId])
  @@index([status])
  @@index([projectType])
  @@map("photo_projects")
}

// PandaCam Photo Model
model Photo {
  id                  String               @id @default(cuid())
  projectId           String               @map("project_id")

  // File info
  fileName            String               @map("file_name")
  fileKey             String               @unique @map("file_key") // S3 key
  fileSize            Int                  @map("file_size") // bytes
  mimeType            String               @map("mime_type")

  // External source tracking (for CompanyCam migration)
  externalId          String?              @map("external_id") // CompanyCam photo ID
  externalSource      String?              @map("external_source") // 'companycam', etc.

  // URLs (generated from fileKey)
  originalUrl         String?              @map("original_url")
  displayUrl          String?              @map("display_url") // 2048px max
  thumbnailUrl        String?              @map("thumbnail_url") // 400px

  // Image dimensions
  width               Int?
  height              Int?
  orientation         Int?                 @default(1) // EXIF orientation 1-8

  // Content
  caption             String?
  description         String?
  photoType           PhotoType            @default(OTHER) @map("photo_type")

  // Location
  capturedAt          DateTime?            @map("captured_at")
  latitude            Float?
  longitude           Float?
  accuracy            Float?               // GPS accuracy in meters

  // Tags and categorization
  tags                String[]             @default([])

  // AI Analysis
  aiLabels            String[]             @default([]) @map("ai_labels")
  aiConfidence        Float[]              @default([]) @map("ai_confidence")
  detectedText        String?              @map("detected_text")
  aiAnalyzedAt        DateTime?            @map("ai_analyzed_at")
  damageDetected      Boolean              @default(false) @map("damage_detected")
  damageSeverity      IssueSeverity?       @map("damage_severity")
  qualityScore        Float?               @map("quality_score") // 0-100

  // Before/After
  beforeAfterId       String?              @map("before_after_id")
  beforeAfterRole     String?              @map("before_after_role") // 'before' or 'after'

  // Relationships
  checklistItemId     String?              @map("checklist_item_id")
  uploadedById        String?              @map("uploaded_by_id")

  // Soft delete
  deletedAt           DateTime?            @map("deleted_at")

  createdAt           DateTime             @default(now()) @map("created_at")
  updatedAt           DateTime             @updatedAt @map("updated_at")

  // Relations
  project             PhotoProject         @relation(fields: [projectId], references: [id], onDelete: Cascade)
  uploadedBy          User?                @relation("PhotoUploadedBy", fields: [uploadedById], references: [id])
  checklistItem       PhotoChecklistItem?  @relation(fields: [checklistItemId], references: [id])
  beforeAfter         BeforeAfterComparison? @relation(fields: [beforeAfterId], references: [id])

  annotations         PhotoAnnotation[]
  galleryPhotos       GalleryPhoto[]

  @@index([projectId])
  @@index([photoType])
  @@index([capturedAt])
  @@index([checklistItemId])
  @@index([beforeAfterId])
  @@index([deletedAt])
  @@index([externalId])
  @@index([externalSource])
  @@map("photos")
}

// PandaCam Photo Annotation Model
model PhotoAnnotation {
  id              String          @id @default(cuid())
  photoId         String          @map("photo_id")

  annotationType  AnnotationType  @map("annotation_type")

  // Position and shape data (stored as JSON for flexibility)
  data            Json            // Contains: x, y, width, height, points, text, etc.

  // Style
  color           String          @default("#FF0000")
  strokeWidth     Int             @default(2) @map("stroke_width")
  fontSize        Int?            @map("font_size")

  // Measurement (if applicable)
  measureValue    Float?          @map("measure_value")
  measureUnit     String?         @map("measure_unit") // ft, in, m, cm

  // Comments
  comment         String?

  createdById     String?         @map("created_by_id")
  createdAt       DateTime        @default(now()) @map("created_at")
  updatedAt       DateTime        @updatedAt @map("updated_at")

  // Relations
  photo           Photo           @relation(fields: [photoId], references: [id], onDelete: Cascade)
  createdBy       User?           @relation("AnnotationCreatedBy", fields: [createdById], references: [id])

  @@index([photoId])
  @@map("photo_annotations")
}

// PandaCam Checklist Model
model PhotoChecklist {
  id              String          @id @default(cuid())
  projectId       String?         @map("project_id")
  templateId      String?         @map("template_id")

  name            String
  description     String?
  status          ChecklistStatus @default(NOT_STARTED)

  // Progress
  totalItems      Int             @default(0) @map("total_items")
  completedItems  Int             @default(0) @map("completed_items")
  progressPercent Float?          @map("progress_percent")

  // Assignment
  assignedToId    String?         @map("assigned_to_id")
  dueDate         DateTime?       @map("due_date")
  completedDate   DateTime?       @map("completed_date")
  completedById   String?         @map("completed_by_id")

  // AI-generated
  isAiGenerated   Boolean         @default(false) @map("is_ai_generated")
  aiSourceType    String?         @map("ai_source_type") // 'voice', 'document', 'page'

  // Export/Signature
  exportedPdfUrl  String?         @map("exported_pdf_url")
  signatureId     String?         @map("signature_id")
  signedAt        DateTime?       @map("signed_at")
  signedById      String?         @map("signed_by_id")

  createdById     String?         @map("created_by_id")
  createdAt       DateTime        @default(now()) @map("created_at")
  updatedAt       DateTime        @updatedAt @map("updated_at")

  // Relations
  project         PhotoProject?   @relation(fields: [projectId], references: [id])
  template        ChecklistTemplate? @relation(fields: [templateId], references: [id])
  assignedTo      User?           @relation("ChecklistAssignedTo", fields: [assignedToId], references: [id])
  completedBy     User?           @relation("ChecklistCompletedBy", fields: [completedById], references: [id])
  createdBy       User?           @relation("ChecklistCreatedBy", fields: [createdById], references: [id])
  signedBy        User?           @relation("ChecklistSignedBy", fields: [signedById], references: [id])

  sections        PhotoChecklistSection[]
  items           PhotoChecklistItem[]

  @@index([projectId])
  @@index([templateId])
  @@index([status])
  @@index([assignedToId])
  @@map("photo_checklists")
}

// PandaCam Checklist Section Model
model PhotoChecklistSection {
  id              String          @id @default(cuid())
