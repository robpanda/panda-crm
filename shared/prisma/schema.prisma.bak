// PandaCam Checklist Item Model
model PhotoChecklistItem {
  id              String              @id @default(cuid())
  checklistId     String              @map("checklist_id")
  sectionId       String?             @map("section_id")

  name            String
  description     String?
  fieldType       ChecklistFieldType  @default(CHECKBOX) @map("field_type")

  sortOrder       Int                 @default(0) @map("sort_order")
  isRequired      Boolean             @default(false) @map("is_required")
  requiresPhoto   Boolean             @default(false) @map("requires_photo")

  // Field options (for MULTIPLE_CHOICE, RATING, etc.)
  options         Json?               // Array of options for choice fields

  // Response data
  isCompleted     Boolean             @default(false) @map("is_completed")
  isNa            Boolean             @default(false) @map("is_na")
  response        String?             // Text response
  responseData    Json?               @map("response_data") // Structured response

  // Rating/Number responses
  ratingValue     Int?                @map("rating_value")
  numberValue     Float?              @map("number_value")

  // Yes/No response
  yesNoValue      Boolean?            @map("yes_no_value")

  // Date response
  dateValue       DateTime?           @map("date_value")

  // Signature
  signatureUrl    String?             @map("signature_url")
  signedAt        DateTime?           @map("signed_at")

  // Notes
  notes           String?

  // Issue tracking
  hasIssue        Boolean             @default(false) @map("has_issue")
  issueSeverity   IssueSeverity?      @map("issue_severity")
  issueNotes      String?             @map("issue_notes")

  completedAt     DateTime?           @map("completed_at")
  completedById   String?             @map("completed_by_id")

  createdAt       DateTime            @default(now()) @map("created_at")
  updatedAt       DateTime            @updatedAt @map("updated_at")

  // Relations
  checklist       PhotoChecklist      @relation(fields: [checklistId], references: [id], onDelete: Cascade)
  section         PhotoChecklistSection? @relation(fields: [sectionId], references: [id])
  completedBy     User?               @relation("ChecklistItemCompletedBy", fields: [completedById], references: [id])

  photos          Photo[]

  @@index([checklistId])
  @@index([sectionId])
  @@index([isCompleted])
  @@map("photo_checklist_items")
}

// PandaCam Checklist Template Model
model ChecklistTemplate {
