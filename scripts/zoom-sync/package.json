{
  "name": "zoom-s3-sync",
  "version": "1.0.0",
  "description": "Sync Zoom cloud recordings to AWS S3",
  "main": "sync-zoom-recordings.js",
  "scripts": {
    "sync": "node sync-zoom-recordings.js",
    "sync:dry-run": "node sync-zoom-recordings.js --dry-run --verbose",
    "sync:verbose": "node sync-zoom-recordings.js --verbose",
    "sync:2024": "node sync-zoom-recordings.js --start-date=2024-01-01 --end-date=2024-12-31",
    "sync:2025": "node sync-zoom-recordings.js --start-date=2025-01-01 --end-date=2025-12-31",
    "sync:2026": "node sync-zoom-recordings.js --start-date=2026-01-01"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/client-secrets-manager": "^3.700.0",
    "playwright": "^1.57.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
